<div class="page-header">
  <center><strong><h1> HRKin Wiki </h1></strong></center>
</div>


<% @pages.each do |page| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to page.title, user_page_path(current_user, page) %>
      </h4>
      <h5 class="media-heading">
        <%= page.content %>
      </h5>

    </div>
     <%#= will_paginate @pages %>
  </div>
  <hr>
<% end %>
